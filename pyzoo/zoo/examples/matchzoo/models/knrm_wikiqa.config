{
  "net_name": "KNRM",
  "global":{
      "model_type": "PY",
      "weights_file": "/home/simanw/MatchZoo/examples/wikiqa/weights/knrm.wikiqa.weights",
      "save_weights_iters": 10,
      "num_iters": 400,
      "display_interval": 10,
      "test_weights_iters": 400,
      "optimizer": "adam",
      "learning_rate": 0.001
  },
  "inputs": {
    "share": {
        "text1_corpus": "/home/simanw/MatchZoo/data/WikiQA/corpus_preprocessed.txt",
        "text2_corpus": "/home/simanw/MatchZoo/data/WikiQA/corpus_preprocessed.txt",
        "use_dpool": false,
        "embed_size": 300,
        "embed_path": "/home/simanw/MatchZoo/data/WikiQA/embed_glove_d300_norm",
        "vocab_size": 18670,
        "train_embed": true,
        "target_mode": "ranking",
        "text1_maxlen": 10,
        "text2_maxlen": 40
    },
    "train": {
        "input_type": "PairGenerator",
        "phase": "TRAIN",
        "use_iter": false,
        "query_per_iter": 50,
        "batch_per_iter": 5,
        "batch_size": 100,
        "relation_file": "/home/simanw/MatchZoo/data/WikiQA/relation_train.txt"
    },
    "valid": {
        "input_type": "ListGenerator",
        "phase": "EVAL",
        "batch_list": 10,
        "relation_file": "/home/simanw/MatchZoo/data/WikiQA/relation_valid.txt"
    },
    "test": {
        "input_type": "ListGenerator",
        "phase": "EVAL",
        "batch_list": 10,
        "relation_file": "/home/simanw/MatchZoo/data/WikiQA/relation_test.txt"
    },
    "predict": {
        "input_type": "ListGenerator",
        "phase": "PREDICT",
        "batch_list": 10,
        "relation_file": "/home/simanw/MatchZoo/data/WikiQA/relation_test.txt"
    }
  },
  "outputs": {
    "predict": {
      "save_format": "TREC",
      "save_path": "predict.test.knrm_ranking.txt"
    }
  },
  "model": {
    "model_path": "/home/simanw/analytics-zoo/pyzoo/zoo/examples/matchzoo/models/",
    "model_py": "knrm.KNRM",
    "setting": {
        "kernel_num": 21,
        "sigma": 0.1,
	    "exact_sigma": 0.001,
	    "dropout_rate": 0.0
    }
  },
  "keras2model": {
    "model_path": "/home/simanw/analytics-zoo/pyzoo/zoo/examples/matchzoo/models/",
    "model_py": "knrm_keras2.KNRM2",
    "setting": {
        "kernel_num": 21,
        "sigma": 0.1,
	    "exact_sigma": 0.001,
	    "dropout_rate": 0.0
    }
  },
  "losses": [
    {
       "object_name": "rank_hinge_loss",
       "object_params": { "margin": 1.0 }
    }
  ],
  "metrics": [ "ndcg@3", "ndcg@5", "map" ]
}
